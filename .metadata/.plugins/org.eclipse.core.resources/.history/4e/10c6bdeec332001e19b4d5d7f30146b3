package com.deepak.searching;

public class Bubblesort {
	public static void main(String[] args) {
		
	
  int arr[]= {12,3,4,5,6,7,19,17,14,16};
  int n = arr.length;
      for(int i=0;i<n-1;i++) 
      {
    	  for(int j=0;i<n-i-1;i++) {
    		  if(arr[j+1]<arr[j]) {
    			 
				int temp=arr[j];
				
				arr[j]=arr[j+1];
				arr[j+1]=temp;
    			 
    			  
    		  }
    	  }
    	  
      }	
	for(int i=0;i<arr[n];i++) {
		System.out.println(arr[i]+ " ");
	}
}
}