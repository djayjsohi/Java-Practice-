package com.deepak.searching;

public class Bubblesort {
	public static void main(String[] args){

  int arr[]= {3,4,5,6,7,19,17,14,16};
      for(int i=0;i<arr.length;i++) 
      {
    	  for(int j=0;i<arr.length-i;j++) {
    		  if(arr[j]>arr[j+1]) {
    			 
				int temp=arr[j];
				arr[j]=arr[j+1];
				arr[j+1]=temp;
    			   
    		  }
    	  }
    	  
      }	
	for(int i=0;i<arr.length;i++){
		System.out.println(arr[i]+ " ");
	}
}
}