package com.example.demo;

public class Fibonacci {
  public static void main(String[] args) {
	  int num1=0, num2=1 , fib;
	  
	  System.out.println(num1);
     System.out.println(num2);
     
     int count=15;
     for(int i=0;i<=count;i++) {
    	 fib=num1+num2;
    	 System.out.println(fib);
    	 num1=num2;
    	 num2=fib;
     }


  }

}
