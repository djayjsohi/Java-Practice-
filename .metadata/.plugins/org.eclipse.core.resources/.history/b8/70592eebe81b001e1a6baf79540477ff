package com.example.demo;

public class Fibonacci {
  public static void main(String[] args) {
	  int num1=0, num2=1 , nth = 0;
	  
	  System.out.println(num1);
     System.out.println(num2);
     
     
     for(int i=0;i<=nth;i++) {
    	 nth=num1+num2;
    	 System.out.println(nth);
    	 num1=num2;
    	 num2=nth;
     }


  }

}
