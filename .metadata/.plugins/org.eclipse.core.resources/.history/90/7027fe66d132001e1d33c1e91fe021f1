package com.deepak.searching;

class Bubblesort{
	public static void main(String[] args) {
		int[] ar= {34,35,56,12,13,4};
		int temp;
		for (int i=0;i<ar.length;i++) {
			for(int j=0;j<ar.length;j++) {
			         if(ar[j]>ar[j+1]) {
			        	temp=ar[j];
			        	ar[j]=ar[j+1];
			        	ar[j+1]=temp;
			         }	
			}
		}
	
	   for(int i=0;i<ar.length;i++) {
		     System.out.println(ar[i]);
	   }
	}
	
}