package com.deepak.searching;

public class Bubblesort {
    public static void main(String[] args){

        int a[]= {36,19,25,16,7};
        ;
        for(int i=0;i<a.length;i++) {
            for(int j=0;i<a.length-i-i;i++) {

                if(a[j]>a[j+1]) {
                    int temp=a[j];
                    a[j]=a[j+1];
                    a[j+1]=temp;
                }
            }

        }
        for(int i=0;i<a.length;i++) {
            System.out.println(a[i]);
        }
    }
}