// suppose want to find nth fibonaci num

 public class Fibonacci{
	 public static void main(String[] args) {
		int a=0;
		int b=1;
		int c;
		 System.out.println("Enter the nth number:" );
		 Scanner sc=new Scanner(System.in);
		 int n =sc.nextInt();
		for(int i=0;i<n;i++) {
			c=a+b;
			a=b;
			b=c;
		}
		System.out.println(a);	
	}
	
 }
